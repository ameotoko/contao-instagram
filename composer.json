{
    "name": "ameotoko/contao-instagram",
    "description": "Instagram for Contao Open Source CMS",
    "keywords": ["contao", "instagram", "feed"],
    "type": "contao-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "https://codefog.pl"
        },
        {
            "name": "Kamil Kuzminski",
            "homepage": "https://github.com/qzminski",
            "role": "developer"
        },
        {
            "name": "Andrey Vinichenko",
            "homepage": "https://github.com/ameotoko"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "contao/core-bundle":"~3.5 || ~4.4",
        "contao-community-alliance/composer-plugin":"^2.4.1 || ~3.0",
        "codefog/contao-haste": "~4.17"
    },
    "conflict": {
        "contao-legacy/cfg_instagram": "*"
    },
    "replace": {
        "contao-legacy/cfg_instagram": "self.version"
    },
    "autoload":{
        "psr-0": {
            "Codefog\\Instagram\\": "src"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/cfg_instagram"
            }
        }
    }
}
